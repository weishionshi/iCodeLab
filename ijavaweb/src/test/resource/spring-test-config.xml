<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:context="http://www.springframework.org/schema/context"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.2.xsd
    ">

    <context:component-scan base-package="com.a3"></context:component-scan>
    
    
    <bean id="child" class="com.a3.javase.chapter5.clazz.staticdemo.Child">
    	<constructor-arg value="name by constructor injection"></constructor-arg>
        <constructor-arg value="1"></constructor-arg>
    </bean>
    
    <bean id="child2" class="com.a3.javase.chapter5.clazz.staticdemo.Child">
    	<property name="name" value="name by property injection"></property>
        <property name="age" value="2"></property>
    </bean>
    
    <bean id="father" class="com.a3.javase.chapter5.clazz.staticdemo.Father" ></bean>

</beans>